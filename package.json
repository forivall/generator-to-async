{
  "name": "generator-to-async",
  "version": "1.0.0",
  "description": "Babel's generator-to-async helper, also with any-promise",
  "license": "ISC",
  "author": "Emily Marigold Klassen <forivall@gmail.com>",
  "homepage": "https://github.com/forivall/generator-to-async",
  "repository": {
    "type": "git",
    "url": "git@github.com:forivall/generator-to-async.git"
  },
  "bugs": {
    "url": "https://github.com/forivall/generator-to-async/issues"
  },
  "scripts": {
    "lint": "xo",
    "lint-staged": "lint-staged",
    "inspect": "if-node-version '>=6' jsinspect",
    "test": "ava",
    "posttest": "npm run lint && npm run inspect",
    "test-cov": "nyc ava",
    "posttest-cov": "npm run posttest",
    "test-ci": "npm run test-cov",
    "posttest-ci": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [],
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "dependencies": {
    "any-promise": "^1.3.0"
  },
  "bundledDependencies": [],
  "peerDependencies": {},
  "devDependencies": {
    "ava": "^0.20.0",
    "coveralls": "^2.11.14",
    "eslint-config-mxo": "^0.5.3",
    "if-node-version": "^1.1.1",
    "jsinspect": "^0.12.6",
    "nyc": "^11.0.3",
    "xo": "^0.18.0"
  },
  "xo": {
    "extends": [
      "mxo/es5"
    ],
    "esnext": false,
    "ignores": [
      "**/fixtures/**"
    ],
    "overrides": [
      {
        "files": [
          "test/**",
          "test.js",
          "test-*.js",
          "**/__tests__/**",
          "**/*.test.js"
        ],
        "extends": [
          "mxo/ava"
        ]
      }
    ]
  }
}
